category:            Language
build-type:          Simple
cabal-version:       >=1.10

extra-source-files:
  .gitignore

library
  exposed-modules:     Language.Untyped.Base
                     , Language.Untyped.Lambda
                     , Language.Untyped.DeBruijn
                     , Language.Untyped.HOAS
                     , Language.Untyped.Lexer
                     , Language.Untyped.Parser
                     , Language.Untyped.Quote
                     , Language.Untyped.QuoteExamples
  -- other-modules:       
  -- other-extensions:    
  build-depends:       base             >=4.10 && <4.11
                     , parsec           >=3.1  && <3.2
                     , syb              >=0.7  && <0.8
                     , template-haskell >=2.12 && <2.13
  hs-source-dirs:      src
  default-language:    Haskell2010
